  <Group id="2" desc="Таблица 8 - Единовременные затраты на ввод в эксплуатацию, тыс.руб.">
    <Param name="costs_before" desc="Затраты до ввода в эксплуатацию, в том числе:" allow_edit="1" units="тыс. руб." formula="%costs_before_land%+%costs_before_networks%+%costs_before_loans%+%costs_before_other%"/>
    <Param name="costs_before_land" desc="Земельный участок" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_before_networks" desc="Подключение к сетям" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_before_loans" desc="Кредиты (проценты по кредитам)" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_before_other" desc="Прочее" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_before_building" desc="Стоимость строительства, в том числе:" allow_edit="1" units="тыс. руб." formula="%costs_before_design%+%costs_before_work%+%costs_before_materials%+%costs_before_equipment%+%costs_before_equipment_installation%+%costs_before_equipment_other%"/>
    <Param name="costs_before_design" desc="затраты на проектирование МКД" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_before_work" desc="затраты на проведение строительно-монтажных работ" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_before_materials" desc="стоимость материалов" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_before_equipment" desc="стоимость оборудования, в том числе" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_before_equipment_energy-efficient" desc="энергоэффективное оборудование" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_before_equipment_installation" desc="монтаж и наладка оборудования" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_before_equipment_other" desc="прочие задачи" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_before_sum_current" desc="СУММА в текущих ценах и без дисконтирования" allow_edit="1" units="тыс. руб." formula="%costs_before%+%costs_before_building%"/>
    <Param name="costs_before_ek" desc="Коэффициент энергоэффективности (Ek)" allow_edit="1" value="1"/>
    <Param name="costs_before_years_building" desc="Количество лет периода (стоительство)" allow_edit="1"/>
    <Param name="costs_before_discount_r" desc="Фактор дисконтирования (R)" allow_edit="1" formula="1/(1+%common_discont_value%)^%costs_before_years_building%"/>
    <Param name="costs_before_sum_accurate" desc="Сумма с учетом поправок" allow_edit="1" units="тыс. руб." formula="%costs_before_sum_current%*%costs_before_discount_r%*%costs_before_ek%"/>
  </Group>
  <Group id="3" desc="Таблица 9 - Единовременные затраты на утилизацию (снос), тыс.руб.">
    <Param name="costs_after" desc="Затраты на снос, в том числе:" allow_edit="1" units="тыс. руб." formula="%costs_after_utilization%+%costs_after_reuse%"/>
    <Param name="costs_after_utilization" desc="стоимость работ по утилизации материалов и конструкций" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_after_reuse" desc="стоимость материалов повторного использования (учитываются со знаком минус)" allow_edit="1" units="тыс. руб."/>
    <Param name="costs_after_sum_current" desc="СУММА в текущих ценах и без дисконтирования" allow_edit="1" units="тыс. руб." formula="%costs_after%"/>
    <Param name="costs_after_years" desc="Номер года прогнозного периода (снос)" allow_edit="1"/>
    <Param name="costs_after_inflation_rate" desc="Среднегодовой темп роста цен (инфляция) за период" allow_edit="1" units="%">
      <value>3</value>
      <value>4</value>
      <value>5</value>
    </Param>
    <Param name="costs_after_inflation_co" desc="Коэффициент роста цен за период" allow_edit="1" formula="(1+%costs_after_inflation_rate%)^%costs_after_years%"/>
    <Param name="costs_after_discount_r" desc="Фактор дисконтирования (R)" allow_edit="1" formula="1/(1+%common_discont_value%)^%costs_after_years%"/>
    <Param name="costs_after_sum_accurate" desc="Сумма с учетом поправок" allow_edit="1" units="тыс. руб." formula="%costs_after%*%costs_before_ek%*costs_after_inflation_co*costs_after_discount_r"/>
  </Group>
  <Group id="4" desc="Таблица 10 - Расходы на коммунальные ресурсы в осенне-зимний период" h1="Стоимость коммунальных ресурсов при поставке из внешних сетей в осенне-зимний период (тариф)" h2="Расчетное потребление ресурсов из внешних сетей, в осенне-зимний период" h3="Расходы на оплату коммунальных ресурсов в осенне-зимний период" h3_formula="h1*h2">
