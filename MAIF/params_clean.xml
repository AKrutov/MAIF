<?xml version="1.0"?>
<ArrayOfGroup xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Group id="1" desc="Исходные данные для расчета">
    <Param name="common_address" desc="Адрес дома" tooltip="Адрес, по которому дом будет расположен" allow_edit="1"/>
    <Param name="common_calc_date" desc="Дата, на которую производятся расчеты" tooltip="Текущая дата" value="%current_date%" allow_edit="0"/>
    <Param name="common_discont_value" desc="Ставка дисконтирования" units="%" tooltip="Должна быть равна ставке ЦБ на текущий год минус инфляция" allow_edit="1" is_numeric="1" is_required="1">
      <value>0</value>
      <value>0.5</value>
      <value>1</value>
      <value>1.5</value>
      <value>8</value>
    </Param>
    <Param name="common_square_full" desc="Площадь дома общая" units="кв.м."  allow_edit="1" is_numeric="1"/>
    <Param name="common_square_useful" desc="Площадь дома полезная" units="кв.м." allow_edit="1"  is_numeric="1"/>
    <Param name="common_square_land" desc="Площадь земельного участка" units="кв.м."  allow_edit="1" is_numeric="1"/>
    <Param name="common_apartments_count" desc="Количество квартир" allow_edit="1"  is_numeric="1"/>
    <Param name="common_people_count" desc="Количество проживающих" units="чел."  allow_edit="1"  is_numeric="1"/>
    <Param name="common_floors_count" desc="Количество этажей"  allow_edit="1"  is_numeric="1"/>
    <Param name="common_build_year" desc="Год постройки"  allow_edit="1"/>
    <Param name="common_energy_class" desc="Класс энергетической эффективности" allow_add="0">
      <value>A++</value>
      <value>A+</value>
      <value>A</value>
      <value>B</value>
      <value>C</value>
      <value>D</value>
      <value>E</value>
      <value>F</value>
      <value>G</value>
    </Param>
    <Param name="common_walls_material" desc="Материал стен" allow_edit="1">
      <value>многослойная теплоэффективная кладка, сборные бетонные блоки для стен подвала с утеплением до глубины промерзания</value>
    </Param>
    <Param name="common_frames_material" desc="Материал перекрытий" allow_edit="1">
      <value>деревянные конструкции с эффективным минераловатным утеплителем, плиты железобетонные пустотные с минераловатным утеплителем</value>
    </Param>
    <Param name="common_base_type" desc="Тип фундамента" allow_edit="1">
      <value>монолитный железобетонный ленточный</value>
    </Param>
    <Param name="common_roof_type" desc="Тип кровли" allow_edit="1">
      <value>скатная по стропильной конструкции из дерева</value>
    </Param>
    <Param name="building_picture" desc="Фото объекта" value="Images\\house-1.png" allow_edit="1" is_hidden="1"/>
  </Group>
  <Group id="2" desc="Единовременные затраты на ввод в эксплуатацию, тыс.руб.">
    <Param name="costs_before" desc="Затраты до ввода в эксплуатацию, в том числе:" units="тыс. руб." allow_edit="1" formula="%costs_before_land%+%costs_before_networks%+%costs_before_loans%+%costs_before_other%"  is_numeric="1"/>
    <Param name="costs_before_land" desc="Земельный участок" units="тыс. руб."  allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_networks" desc="Подключение к сетям" units="тыс. руб."  allow_edit="1" is_required="1" is_numeric="1"/>
    <Param name="costs_before_loans" desc="Кредиты (проценты по кредитам)" units="тыс. руб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_other" desc="Прочее" units="тыс. руб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_building" desc="Стоимость строительства, в том числе:" units="тыс. руб."  allow_edit="1" formula="%costs_before_design%+%costs_before_work%+%costs_before_materials%+%costs_before_equipment%+%costs_before_equipment_installation%+%costs_before_equipment_other%"  is_numeric="1"/>
    <Param name="costs_before_design" desc="затраты на проектирование МКД" units="тыс. руб."  allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_work" desc="затраты на проведение строительно-монтажных работ" units="тыс. руб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_materials" desc="стоимость материалов" units="тыс. руб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_equipment" desc="стоимость оборудования, в том числе" units="тыс. руб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_equipment_energy-efficient" desc="энергоэффективное оборудование" units="тыс. руб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_equipment_installation" desc="монтаж и наладка оборудования" units="тыс. руб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_equipment_other" desc="прочие задачи" units="тыс. руб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_sum_current" desc="СУММА в текущих ценах и без дисконтирования" units="тыс. руб." allow_edit="1" formula="%costs_before%+%costs_before_building%" is_hidden="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_ek" desc="Коэффициент энергоэффективности (Ek)" value="1" allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_years_building" desc="Количество лет периода (стоительство)" allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_before_discount_r" desc="Фактор дисконтирования (R)" is_required="1" allow_edit="1" is_hidden="1" formula="1/Math.Pow((1+%common_discont_value%),%costs_before_years_building%)" is_numeric="1"/>
    <Param name="costs_before_sum_accurate" desc="Сумма с учетом поправок" is_required="1" allow_edit="1" units="тыс. руб." is_hidden="1" formula="(%costs_before_sum_current%)*(%costs_before_discount_r%)*%costs_before_ek%" is_numeric="1"/>
  </Group>
  <Group id="3" desc="Единовременные затраты на утилизацию (снос), тыс.руб.">
    <Param name="costs_after" desc="Затраты на снос, в том числе:" units="тыс. руб." allow_edit="1" formula="%costs_after_utilization%+%costs_after_reuse%" is_required="1"  is_numeric="1"/>
    <Param name="costs_after_utilization" desc="стоимость работ по утилизации материалов и конструкций" units="тыс. руб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_after_reuse" desc="стоимость материалов повторного использования (учитываются со знаком минус)" units="тыс. руб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_after_sum_current" desc="СУММА в текущих ценах и без дисконтирования" units="тыс. руб." allow_edit="1" formula="%costs_after%" is_hidden="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_after_years" desc="Номер года прогнозного периода (снос)" allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_after_inflation_rate" desc="Среднегодовой темп роста цен (инфляция) за период" units="%" value="0.04" allow_edit="1" is_required="1" is_numeric="1">
      <value>3</value>
      <value>4</value>
      <value>5</value>
    </Param>
    <Param name="costs_after_inflation_co" desc="Коэффициент роста цен за период" is_required="1" allow_edit="1" is_hidden="1" formula="Math.Pow((1+%costs_after_inflation_rate%),%costs_after_years%)" is_numeric="1"/>
    <Param name="costs_after_discount_r" desc="Фактор дисконтирования (R)" is_required="1" allow_edit="1" is_hidden="1" formula="1/(Math.Pow((1+%common_discont_value%),%costs_after_years%))" is_numeric="1"/>
    <Param name="costs_after_sum_accurate" desc="Сумма с учетом поправок" units="тыс. руб." allow_edit="1" formula="%costs_after%*%costs_before_ek%*%costs_after_inflation_co%*%costs_after_discount_r%" is_hidden="1" is_required="1"  is_numeric="1"/>
  </Group>
  <Group id="4" desc="Расходы на коммунальные ресурсы в осенне-зимний период" h1="Стоимость коммунальных ресурсов при поставке из внешних сетей в осенне-зимний период (тариф)" h2="Расчетное потребление ресурсов из внешних сетей, в осенне-зимний период" h3="Расходы на оплату коммунальных ресурсов в осенне-зимний период" h3_formula="h1*h2">
    <Param name="costs_communal_fw_electricity" desc="Электроэнергия" units="руб./кВт.ч" allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_fw_gas" desc="Газ" units="руб./м куб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_fw_solid" desc="Твердое топливо" units="руб./тн" allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_fw_heating" desc="Отопление" units="руб./Гкал" allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_fw_gvs" desc="ГВС" units="руб./м куб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_fw_hvs" desc="ХВС" units="руб./м куб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_fw_sewerage" desc="Канализация (водоотведение)" units="руб./м куб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_fw_sum_current" desc="СУММА" units="руб." allow_edit="1" formula="%costs_communal_fw_electricity[2]%+%costs_communal_fw_gas[2]%+%costs_communal_fw_solid[2]%+%costs_communal_fw_heating[2]%+%costs_communal_fw_gvs[2]%+%costs_communal_fw_hvs[2]%+%costs_communal_fw_sewerage[2]%" is_hidden="1" is_required="1" scope="h1" is_numeric="1"/>
    <Param name="costs_communal_fw_sum" desc="Сумма за осенне-зимний период" units="тыс. руб." allow_edit="1" formula="%costs_communal_fw_sum_current%/1000" is_hidden="1" is_required="1" scope="h1" is_numeric="1"/>
  </Group>
  <Group id="5" desc="Расходы на коммунальные ресурсы в весенне-летний период" h1="Стоимость коммунальных ресурсов при поставке из внешних сетей в весенне-летний период (тариф)" h2="Расчетное потребление ресурсов из внешних сетей, в весенне-летний период" h3="Расходы на оплату коммунальных ресурсов в весенне-летний период" h3_formula="h1*h2">
    <Param name="costs_communal_ss_electricity" desc="Электроэнергия" units="руб./кВт.ч" allow_edit="1" is_required="1" is_numeric="1"/>
    <Param name="costs_communal_ss_gas" desc="Газ" units="руб./м куб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_ss_solid" desc="Твердое топливо" units="руб./тн" allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_ss_heating" desc="Отопление" units="руб./Гкал" allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_ss_gvs" desc="ГВС" units="руб./м куб." allow_edit="1" is_required="1" is_numeric="1"/>
    <Param name="costs_communal_ss_hvs" desc="ХВС" units="руб./м куб." allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_ss_sewerage" desc="Канализация (водоотведение)" units="руб./м куб." allow_edit="1" is_required="1" is_numeric="1"/>
    <Param name="costs_communal_ss_sum_current" desc="СУММА" scope="h1" is_hidden="1" is_required="1" allow_edit="1" units="руб." formula="%costs_communal_ss_electricity[2]%+%costs_communal_ss_gas[2]%+%costs_communal_ss_solid[2]%+%costs_communal_ss_heating[2]%+%costs_communal_ss_gvs[2]%+%costs_communal_ss_hvs[2]%+%costs_communal_ss_sewerage[2]%" is_numeric="1"/>
    <Param name="costs_communal_ss_sum" desc="Сумма за весенне-летний период" scope="h1" is_required="1" allow_edit="1" is_hidden="1" units="тыс. руб." formula="(%costs_communal_ss_sum_current%)/1000" is_numeric="1"/>
  </Group>
  <Group id="6" desc="Расходы на коммунальные ресурсы, тыс. руб." h1="Расходы на оплату коммунальных ресурсов в месяц" h2="Количество месяцев в периоде" h3="Сумма" h3_formula="h1*h2">
    <Param name="costs_communal_sum_fw" is_hidden="1" desc="Осенне-зимний период" units="тыс. руб." formula="%costs_communal_fw_sum%;7;%costs_communal_fw_sum%*7" allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_sum_ss" is_hidden="1" desc="Весенне-летний период" units="тыс. руб." formula="%costs_communal_ss_sum%;5;%costs_communal_ss_sum%*5" allow_edit="1" is_required="1"  is_numeric="1"/>
    <Param name="costs_communal_sum_1" desc="СУММА за 12 месяцев" units="тыс. руб." is_hidden="1" formula="%costs_communal_sum_fw[2]%+%costs_communal_sum_ss[2]%" is_required="1" scope="h1"  is_numeric="1"/>
    <Param name="costs_communal_sum_30" desc="СУММА за 30 лет (в текущих ценах и без дисконтирования)" units="тыс. руб." allow_edit="1" formula="%costs_communal_sum_1%*30" is_hidden="1" is_required="1" scope="h1"  is_numeric="1"/>
    <Param name="costs_communal_sum_gk" desc="Коэффициент зелености (Gk)" value="1" allow_edit="1" is_required="1" scope="h1"  is_numeric="1"/>
    <Param name="costs_communal_sum_inflation_rate" desc="Среднегодовой темп роста цен (инфляция) за период" units="%" allow_edit="1" is_required="1" scope="h1" value="0.04"  is_numeric="1">
      <value>3</value>
      <value>4</value>
      <value>5</value>
    </Param>
    <Param name="costs_communal_sum_inflation_co" desc="Коэффициент роста цен за период" scope="h1" is_hidden="1" is_required="1" allow_edit="1" formula="Math.Pow((1+%costs_communal_sum_30%),30)" is_numeric="1"/>
    <Param name="costs_communal_sum_discount_r" desc="Фактор дисконтирования (R)" scope="h1" is_hidden="1" is_required="1" allow_edit="1" formula="1/Math.Pow((1+%common_discont_value%),30)" is_numeric="1"/>
    <Param name="costs_communal_sum_accurate" desc="Расходы на коммунальные ресурсы за 30 лет" units="тыс. руб." allow_edit="1" formula="%costs_communal_sum_1%*%costs_communal_sum_gk%*%costs_communal_sum_discount_r%" is_hidden="1" is_required="1" scope="h1"  is_numeric="1"/>
  </Group>
  <Group id="7" desc="Расходы на капитальный ремонт общего имущества в МКД (30 лет), тыс. руб." h1="Стоимость капитального ремонта общего имущества, тыс. рублей" h2="Периодичность капитального ремонта общего имущества, лет" h3="Расходы на капитальный ремонт общего имущества (30 лет), тыс. руб." h3_formula="(30/h2)*h1">
    <Param name="costs_repair_foundation" desc="фундамент" units="тыс. руб." value=";30;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_walls" desc="стены и элементы фасада" units="тыс. руб." value=";30;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_roof" desc="крыша" units="тыс. руб." value=";30;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_common" desc="помещения общего пользования и подвал" units="тыс. руб." value=";30;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_garbage" desc="мусоропроводы" units="тыс. руб." value=";10;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_lifts" desc="лифты" units="тыс. руб." value=";10;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_ventilation" desc="система вентиляции и дымоудаления" units="тыс. руб." value=";15;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_electricity" desc="система электроснабжения" units="тыс. руб." value=";15;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_gas" desc="система газоснабжения" units="тыс. руб." value=";20;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_heating" desc="система отопления" units="тыс. руб." value=";15;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_water" desc="система водоснабжения" units="тыс. руб." value=";15;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_drainage" desc="система водоотведения" units="тыс. руб." value=";10;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_sum" desc="СУММА за 30 лет (в текущих ценах и без дисконтирования)" units="тыс. руб." allow_edit="1" formula="%costs_repair_foundation[2]%+%costs_repair_walls[2]%+%costs_repair_roof[2]%+%costs_repair_common[2]%+%costs_repair_garbage[2]%+%costs_repair_lifts[2]%+%costs_repair_ventilation[2]%+%costs_repair_electricity[2]%+%costs_repair_gas[2]%+%costs_repair_heating[2]%+%costs_repair_water[2]%+%costs_repair_drainage[2]%" is_hidden="1" is_required="1" scope="h1"  is_numeric="1"/>
    <Param name="costs_repair_sum_gk" desc="Коэффициент зелености (Gk)" value="1" allow_edit="1" is_required="1" scope="h1"  is_numeric="1"/>
    <Param name="costs_repair_sum_inflation_rate" desc="Среднегодовой темп роста цен (инфляция) за период" units="%" allow_edit="1" is_required="1" scope="h1" value="0.04" >
      <value>3</value>
      <value>4</value>
      <value>5</value>
    </Param>
    <Param name="costs_repair_avg_period" is_hidden="1" scope="h1" is_required="1" desc="Средняя периодичность ремонта общего имущества в МКД, лет" allow_edit="1" formula="(%costs_repair_foundation[1]%+%costs_repair_walls[1]%+%costs_repair_roof[1]%+%costs_repair_common[1]%+%costs_repair_garbage[1]%+%costs_repair_lifts[1]%+%costs_repair_ventilation[1]%+%costs_repair_electricity[1]%+%costs_repair_gas[1]%+%costs_repair_heating[1]%+%costs_repair_water[1]%+%costs_repair_drainage[1]%)/12" is_numeric="1" units="лет"/>
    <Param name="costs_repair_sum_inflation_co" is_hidden="1" scope="h1" is_required="1" desc="Коэффициент роста цен за период" allow_edit="1" formula="Math.Pow((1+%costs_repair_sum_inflation_rate%),%costs_repair_avg_period%)" is_numeric="1"/>
    <Param name="costs_repair_sum_discount_r" is_hidden="1" scope="h1" is_required="1" desc="Фактор дисконтирования (R)" allow_edit="1" formula="1/(Math.Pow((1+%common_discont_value%),%costs_repair_avg_period%))" is_numeric="1"/>
    <Param name="costs_repair_sum_accurate" desc="Сумма с учетом поправок" units="тыс. руб." allow_edit="1" formula="%costs_repair_sum%*%costs_repair_sum_gk%*%costs_repair_sum_inflation_co%*%costs_repair_sum_discount_r%" is_hidden="1" is_required="1" scope="h1"  is_numeric="1"/>
  </Group>
  <Group id="8" desc="Расходы на текущий (плановый) ремонт общего имущества за 30 лет, тыс. руб." h1="Стоимость периодического ремонта общего имущества в МКД, тыс. рублей" h2="Периодичность ремонта общего имущества в МКД, лет" h3="Расходы на текущий ремонт общего имущества в МКД (30 лет), тыс. руб." h3_formula="(30/h2)*h1">
    <Param name="costs_repair_plan_foundation" desc="фундамент" units="тыс. руб." value=";10;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_plan_walls" desc="стены и элементы фасада" units="тыс. руб." value=";10;" allow_edit="1"  is_numeric="1"/>
    <Param name="costs_repair_plan_roof" desc="крыша" units="тыс. руб." value=";10;" allow_edit="1"  is_numeric="1"/>
